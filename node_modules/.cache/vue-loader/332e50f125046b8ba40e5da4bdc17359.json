re()\n    const router = useRouter()\n\n    const tasks = computed(() => store.getters.tasks)\n    const activeTasksCount = computed(() => store.getters.activeTasksCount)\n\n    return {\n      tasks, activeTasksCount,\n      open: id => router.push(`/task/${id}`)\n    }\n  },\n  components: {AppStatus}\n}\n</script>\n"]}]}